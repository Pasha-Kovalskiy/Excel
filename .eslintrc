{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "linebreak-style":"off",
        "max-len":"off",
        "eol-last":"off",
        "require-jsdoc":"off",
        "comma-dangle":"off"
    },
    "extends": ["eslint:recommended", "google"],
    "indent": ["error", "tab"]
}